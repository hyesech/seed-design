{
  "name": "@seed-design/react-button",
  "version": "1.0.0-beta.0",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    }
  },
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "yarn build:js && yarn build:dts",
    "build:dts": "tsc --emitDeclarationOnly",
    "build:js": "node ./esbuild.config.js",
    "clean": "rimraf lib",
    "dev": "yarn build:js --watch && yarn build:dts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/seed-design.git",
    "directory": "packages/react/button"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0"
  },
  "devDependencies": {
    "@seed-design/stylesheet": "^1.0.0-beta.4",
    "esbuild": "^0.15.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "ultra-runner": "^3.10.5"
  },
  "dependencies": {
    "@seed-design/react-shared": "^1.0.0-beta.0",
    "@seed-design/react-text": "^1.0.0-beta.0",
    "change-case": "^4.1.2",
    "clsx": "^1.2.1"
  },
  "ultra": {
    "concurrent": [
      "dev",
      "build"
    ]
  }
}
